<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="fr.labri.harmony.analysis.migration.scanlib">
	<class name="Migration">
		<id name="id">
			<generator class="native" />
		</id>
		
		<property name="project" not-null="true" />
		
		<property name="step" not-null="true" />	
			
		<property name="source" not-null="true" />
		
		<property name="target" not-null="true" />
		
	</class>

</hibernate-mapping>